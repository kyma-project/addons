{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "region": {
      "type": "string",
      "oneOf": [
        {
          "enum": ["us-east-1"],
          "title": "US East (N. Virginia)"
        },
        {
          "enum": ["us-east-2"],
          "title": "US East (Ohio)"
        },
        {
          "enum": ["us-west-1"],
          "title": "US West (N. California)"
        },
        {
          "enum": ["us-west-2"],
          "title": "US West (Oregon)"
        },
        {
          "enum": ["ca-central-1"],
          "title": "Canada (Central)"
        },
        {
          "enum": ["eu-central-1"],
          "title": "EU (Frankfurt)"
        },
        {
          "enum": ["eu-west-1"],
          "title": "EU (Ireland)"
        },
        {
          "enum": ["eu-west-2"],
          "title": "EU (London)"
        },
        {
          "enum": ["eu-west-3"],
          "title": "EU (Paris)"
        },
        {
          "enum": ["eu-north-1"],
          "title": "EU (Stockholm)"
        },
        {
          "enum": ["ap-east-1"],
          "title": "Asia Pacific (Hong Kong)"
        },
        {
          "enum": ["ap-northeast-1"],
          "title": "Asia Pacific (Tokyo)"
        },
        {
          "enum": ["ap-northeast-2"],
          "title": "Asia Pacific (Seoul)"
        },
        {
          "enum": ["ap-northeast-3"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-1"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-2"],
          "title": "Asia Pacific (Sydney)"
        },
        {
          "enum": ["ap-south-1"],
          "title": "Asia Pacific (Mumbai)"
        },
        {
          "enum": ["sa-east-1"],
          "title": "South America (São Paulo)"
        }
      ],
      "default": "us-east-1",
      "title": "Broker region"
    },
    "brokerid": {
      "type": "string",
      "title": "Service Broker ID",
      "default": "awsservicebroker"
    },
    "s3region": {
      "type": "string",
      "oneOf": [
        {
          "enum": ["us-east-1"],
          "title": "US East (N. Virginia)"
        },
        {
          "enum": ["us-east-2"],
          "title": "US East (Ohio)"
        },
        {
          "enum": ["us-west-1"],
          "title": "US West (N. California)"
        },
        {
          "enum": ["us-west-2"],
          "title": "US West (Oregon)"
        },
        {
          "enum": ["ca-central-1"],
          "title": "Canada (Central)"
        },
        {
          "enum": ["eu-central-1"],
          "title": "EU (Frankfurt)"
        },
        {
          "enum": ["eu-west-1"],
          "title": "EU (Ireland)"
        },
        {
          "enum": ["eu-west-2"],
          "title": "EU (London)"
        },
        {
          "enum": ["eu-west-3"],
          "title": "EU (Paris)"
        },
        {
          "enum": ["eu-north-1"],
          "title": "EU (Stockholm)"
        },
        {
          "enum": ["ap-east-1"],
          "title": "Asia Pacific (Hong Kong)"
        },
        {
          "enum": ["ap-northeast-1"],
          "title": "Asia Pacific (Tokyo)"
        },
        {
          "enum": ["ap-northeast-2"],
          "title": "Asia Pacific (Seoul)"
        },
        {
          "enum": ["ap-northeast-3"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-1"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-2"],
          "title": "Asia Pacific (Sydney)"
        },
        {
          "enum": ["ap-south-1"],
          "title": "Asia Pacific (Mumbai)"
        },
        {
          "enum": ["sa-east-1"],
          "title": "South America (São Paulo)"
        }
      ],
      "default": "us-east-1",
      "title": "S3 Bucket region"
    },
    "bucket": {
      "type": "string",
      "title": "S3 Bucket name",
      "default": "awsservicebroker"
    },
    "key": {
      "type": "string",
      "title": "S3 Bucket key",
      "default": "templates/latest"
    },
    "tablename": {
      "type": "string",
      "title": "DynamoDB table name",
      "default": "awssb"
    },
    "imagePullPolicy": {
      "type": "string",
      "enum": ["Always", "IfNotPresent", "Never"],
      "default": "IfNotPresent",
      "title": "Image pull policy"
    },
    "prescribeoverrides": {
      "title": "Prescribe-overrides feature enabled",
      "type": "boolean",
      "default": true
    },
    "targetaccountid": {
      "type": "string",
      "title": "Target account ID"
    },
    "targetrolename": {
      "type": "string",
      "title": "Target IAM role name"
    },
    "vpcid": {
      "type": "string",
      "title": "VPC ID"
    }
  },
  "required": ["region", "s3region", "clusterDomain", "bucket", "tablename", "key", "brokerid"]
}
