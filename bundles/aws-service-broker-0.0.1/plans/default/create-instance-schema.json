{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "imagePullPolicy": {
      "type": "string",
      "enum": ["Always", "IfNotPresent", "Never"],
      "default": "IfNotPresent",
      "title": "Image pull policy"
    },
    "region": {
      "type": "string",
      "oneOf": [
        {
          "enum": ["us-east-1"],
          "title": "US East (N. Virginia)"
        },
        {
          "enum": ["us-east-2"],
          "title": "US East (Ohio)"
        },
        {
          "enum": ["us-west-1"],
          "title": "US West (N. California)"
        },
        {
          "enum": ["us-west-2"],
          "title": "US West (Oregon)"
        },
        {
          "enum": ["ca-central-1"],
          "title": "Canada (Central)"
        },
        {
          "enum": ["eu-central-1"],
          "title": "EU (Frankfurt)"
        },
        {
          "enum": ["eu-west-1"],
          "title": "EU (Ireland)"
        },
        {
          "enum": ["eu-west-2"],
          "title": "EU (London)"
        },
        {
          "enum": ["eu-west-3"],
          "title": "EU (Paris)"
        },
        {
          "enum": ["eu-north-1"],
          "title": "EU (Stockholm)"
        },
        {
          "enum": ["ap-east-1"],
          "title": "Asia Pacific (Hong Kong)"
        },
        {
          "enum": ["ap-northeast-1"],
          "title": "Asia Pacific (Tokyo)"
        },
        {
          "enum": ["ap-northeast-2"],
          "title": "Asia Pacific (Seoul)"
        },
        {
          "enum": ["ap-northeast-3"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-1"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-2"],
          "title": "Asia Pacific (Sydney)"
        },
        {
          "enum": ["ap-south-1"],
          "title": "Asia Pacific (Mumbai)"
        },
        {
          "enum": ["sa-east-1"],
          "title": "South America (São Paulo)"
        }
      ],
      "default": "eu-central-1",
      "title": "AWS broker region",
      "description": "The region of the AWS service broker"
    },
    "s3region": {
      "type": "string",
      "oneOf": [
        {
          "enum": ["us-east-1"],
          "title": "US East (N. Virginia)"
        },
        {
          "enum": ["us-east-2"],
          "title": "US East (Ohio)"
        },
        {
          "enum": ["us-west-1"],
          "title": "US West (N. California)"
        },
        {
          "enum": ["us-west-2"],
          "title": "US West (Oregon)"
        },
        {
          "enum": ["ca-central-1"],
          "title": "Canada (Central)"
        },
        {
          "enum": ["eu-central-1"],
          "title": "EU (Frankfurt)"
        },
        {
          "enum": ["eu-west-1"],
          "title": "EU (Ireland)"
        },
        {
          "enum": ["eu-west-2"],
          "title": "EU (London)"
        },
        {
          "enum": ["eu-west-3"],
          "title": "EU (Paris)"
        },
        {
          "enum": ["eu-north-1"],
          "title": "EU (Stockholm)"
        },
        {
          "enum": ["ap-east-1"],
          "title": "Asia Pacific (Hong Kong)"
        },
        {
          "enum": ["ap-northeast-1"],
          "title": "Asia Pacific (Tokyo)"
        },
        {
          "enum": ["ap-northeast-2"],
          "title": "Asia Pacific (Seoul)"
        },
        {
          "enum": ["ap-northeast-3"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-1"],
          "title": "Asia Pacific (Osaka-Local)"
        },
        {
          "enum": ["ap-southeast-2"],
          "title": "Asia Pacific (Sydney)"
        },
        {
          "enum": ["ap-south-1"],
          "title": "Asia Pacific (Mumbai)"
        },
        {
          "enum": ["sa-east-1"],
          "title": "South America (São Paulo)"
        }
      ],
      "default": "eu-central-1",
      "title": "S3 bucket region",
      "description": "The region of the S3 bucket"
    },
    "bucket": {
      "type": "string",
      "title": "Bucket name",
      "default": "awsservicebroker",
      "description": "The name of the S3 bucket"
    },
    "key": {
      "type": "string",
      "title": "Bucket key",
      "default": "templates/latest",
      "description": "The name of the bucket key"
    },
    "tablename": {
      "type": "string",
      "title": "Table name",
      "default": "awssb",
      "description": "The name of the table"
    },
    "targetaccountid": {
      "type": "string",
      "title": "Target account ID",
      "description": "The ID of your target account"
    },
    "targetrolename": {
      "type": "string",
      "title": "Target role name",
      "description": "The name of your target role"
    },
    "vpcid": {
      "type": "string",
      "title": "VPC ID",
      "description": "The ID of your VPC"
    },
  },
  "required": ["region", "s3region", "clusterDomain", "bucket", "tablename"]
}
